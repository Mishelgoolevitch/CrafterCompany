@using CrafterCompany.Domain.Repositories.Abstract
@model IEnumerable<EquipmentDTO>
@inject IEquipmentsRepository equipmentRepository
@await Html.PartialAsync("_SidebarPartial")
		
		<li>
	<a asp-area="" asp-controller="Equipments" asp-action="Index">Услуги</a>
			<ul>
				@foreach (EquipmentDTO entity in Model)
				{
					<li>
						<a asp-area="" asp-controller="Equipments" asp-action="Show" asp-route-id="@entity.Model">
							@entity.PhotoFileName
						</a>
					</li>
				}
			</ul>
		</li>
		
